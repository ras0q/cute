Set Shell "bash"
Set FontSize 24
Set Width 1600
Set Height 800
Set Margin 40
Set MarginFill "#6B50FF"
Set BorderRadius 20

Env CUTE_DIR ".."

Sleep 2s Type "source $CUTE_DIR/cute"
Sleep 1s Enter

Sleep 1s Type "cute "
Sleep 1s Tab 2

Sleep 1s Type "bu"
Sleep 1s Tab
Sleep 1s Enter

Sleep 10s
